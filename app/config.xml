<?xml version='1.0' encoding='utf-8'?>
<widget id="com.couponbooked.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Coupon Booked</name>
    <icon src="testingIcon.png" />
    <author email="ajmeese7@gmail.com" href="http://aaronmeese.com">
        Aaron Meese
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="19" />
    <platform name="android">
    </platform>
    <access origin="*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <preference name="AndroidLaunchMode" value="singleTask" />
    <plugin name="cordova-plugin-safariviewcontroller" spec="^1.6.0" />
    <plugin name="cordova-plugin-customurlscheme" spec="~4.2.0">
        <variable name="URL_SCHEME" value="com.couponbooked.app" />
        <variable name="ANDROID_SCHEME" value="com.couponbooked.app" />
        <variable name="ANDROID_HOST" value="couponbooked.auth0.com" />
        <variable name="ANDROID_PATHPREFIX" value="/cordova/com.couponbooked.app/callback" />
    </plugin>
    <plugin name="cordova-plugin-inappbrowser" spec="~1.3.0" />
    <plugin name="cordova-plugin-safariviewcontroller" spec="~1.4.6" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.1" />
    <plugin name="cordova-plugin-screen-orientation" spec="^3.0.2" />
    <engine name="android" spec="^7.1.1" />
</widget>
